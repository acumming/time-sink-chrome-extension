{
    "manifest_version": 2,
    "name": "Time Sink URL Tracker",
    "description": "Appends [[domain.com]] to the end of HTML title elements.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "time_sink.js"
            ]
        }
    ]
}
